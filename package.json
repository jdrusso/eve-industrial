{
  "name": "Shekelator",
  "author": {
    "name": "Jesus Rambo",
    "email": "thejesusrambo@gmail.com"
  },
  "description":"Shekelator desktop app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "fetch": "^1.1.0",
    "material-ui": "^0.20.2",
    "python-shell": "^1.0.6",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-scripts": "2.1.2"
  },
  "derefSymlinks": false,
  "asar":false,
  "asdfasdfa":false,
  "scripts": {
    "start": "nf start -p 3000",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "electron-start": "node src/start-react",
    "react-start": "BROWSER=none react-scripts start",
    "pack": "build --dir",
    "dist": "npm run build && build",
    "postinstall": "install-app-deps"
  },
  "homepage": "./",
  "main": "src/start.js",
  "build": {
    "asar":false,
    "appId": "com.electron.electron-shekelator",
    "linux": {
      "target": [
        "AppImage"
      ],
      "category":"Utility"
    },
    "directories": {
      "buildResources": "public"
    },
    "extraFiles": ["public/eve_utils.py",
              "public/modifiers.json",
              "public/index.html",
              "public/industryActivityMaterials.csv",
              "public/engine.py",
              "public/shekelator/*",
              "public/shekelator/eve-utils/eve_utils.py"]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "electron": "^4.0.0",
    "electron-builder": "^20.38.4"
  },
  "proxy": "http://localhost:5000"
}
